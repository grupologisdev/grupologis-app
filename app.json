{
  "expo": {
    "name": "grupologis-app",
    "slug": "grupologis-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "permissions": [
      "READ_EXTERNAL_STORAGE",
      "WRITE_EXTERNAL_STORAGE",
      "MEDIA_LIBRARY"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSFileProviderServiceName": "com.myappglogis.fileprovider",
        "NSFileProtection": "CompleteUnlessOpen",
        "UISupportedInterfaceOrientations": ["UIInterfaceOrientationPortrait"],
        "NSPhotoLibraryUsageDescription": "La aplicaci√≥n necesita acceder a la biblioteca de medios para guardar los archivos descargados"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.glogis2023.grupologisapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "00c0fd79-69a5-4fc0-9d1e-c64f5d218927"
      }
    }
  }
}
